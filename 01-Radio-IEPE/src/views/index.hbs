
<main class="index">
    <header class="masthead  d-flex">
        <div class="container-fuid text-center my-auto masthead-background">
            
            <h2 class="mb-1 nombre">{{nombre}}</h2>

            <div class="centrar-texto radio m-2">
        <div class="pt-5">
        <audio controls>
            <source src="http://stream.zeno.fm/523mrpp6vwzuv.ogg" type="audio/ogg">
            <source src="http://stream.zeno.fm/523mrpp6vwzuv.mpeg" type="audio/mpeg">
            <source src="http://stream.zeno.fm/523mrpp6vwzuv.wav" type="audio/wav">
                <object type="application/x-shockwave-flash" data="player.swf?miArchivo=mi-archivo-de-audio.mp3">
                    <param name="movie" value="player.swf?miArchivo=mi-archivo-de-audio.mp3">
                    <a href="http://stream.zeno.fm/523mrpp6vwzuv.mp3">Descárgate aquí mi archivo de audio</a>
                </object>
        </audio>
        </div>
{{!--  --}}
 <div class="container p-4">
    <div class="row">
        {{#each linksIndex}}
        <div class="col-md-3">
            <div class="card text-center">
                <div class="card-body">
                    <a href="#">
                        <h3 class="card-title text-uppercase">
                            {{title}}
                        </h3>
                    </a>
                    <p class="m-2">{{description}}</p>
                    <p class="text-muted">
                        {{timeago created_at}}
                    </p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div> 
